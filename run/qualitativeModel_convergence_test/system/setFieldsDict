FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue p 0
    volScalarFieldValue T 0.3
    volScalarFieldValue Y 0
);

regions
(
    boxToCell
    {
        box (0 -1 -1) (1 2 2);
        fieldValues
        (
            volScalarFieldValue p 2
            volScalarFieldValue T 3
            volScalarFieldValue Y 1
        );
    }
);

