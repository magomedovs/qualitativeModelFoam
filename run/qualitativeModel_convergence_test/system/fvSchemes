FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    // default         backward; // Euler backward
    default         CrankNicolson 1;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         Gauss linear; //upwind linear vanLeer
    // div(phiOneEpsUBy2,U)  Gauss vanLeer;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
    // reconstruct(oneEpsUBy2) vanLeer;
}

snGradSchemes
{
    default         orthogonal;
}


