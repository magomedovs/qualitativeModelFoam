type            sets;
setFormat raw;

start   (0 0 0);
end     (200 0 0);
fields  (U T Y omega);

setConfig
{
    axis    x;
}

interpolationScheme     cellPointFace; //cellPointFace cellPoint cell

sets
(
    line
    {
        type     face; //midPoint midPointAndFace face

        $setConfig;
        start $start;
        end   $end;
    }
);


